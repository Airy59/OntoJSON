<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/person#"
     xml:base="http://example.org/person"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    
    <!-- Ontology Declaration -->
    <owl:Ontology rdf:about="http://example.org/person">
        <owl:versionInfo>1.0.0</owl:versionInfo>
        <rdfs:comment>A simple ontology for describing persons and their relationships</rdfs:comment>
    </owl:Ontology>
    
    <!-- Classes -->
    
    <!-- Person Class -->
    <owl:Class rdf:about="http://example.org/person#Person">
        <rdfs:label xml:lang="en">Person</rdfs:label>
        <rdfs:comment xml:lang="en">A human being</rdfs:comment>
    </owl:Class>
    
    <!-- Employee Class (subclass of Person) -->
    <owl:Class rdf:about="http://example.org/person#Employee">
        <rdfs:subClassOf rdf:resource="http://example.org/person#Person"/>
        <rdfs:label xml:lang="en">Employee</rdfs:label>
        <rdfs:comment xml:lang="en">A person who works for an organization</rdfs:comment>
    </owl:Class>
    
    <!-- Address Class -->
    <owl:Class rdf:about="http://example.org/person#Address">
        <rdfs:label xml:lang="en">Address</rdfs:label>
        <rdfs:comment xml:lang="en">A postal address</rdfs:comment>
    </owl:Class>
    
    <!-- Organization Class -->
    <owl:Class rdf:about="http://example.org/person#Organization">
        <rdfs:label xml:lang="en">Organization</rdfs:label>
        <rdfs:comment xml:lang="en">A company or institution</rdfs:comment>
    </owl:Class>
    
    <!-- Datatype Properties -->
    
    <!-- firstName property -->
    <owl:DatatypeProperty rdf:about="http://example.org/person#firstName">
        <rdfs:domain rdf:resource="http://example.org/person#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">First Name</rdfs:label>
        <rdfs:comment xml:lang="en">The person's given name</rdfs:comment>
    </owl:DatatypeProperty>
    
    <!-- lastName property -->
    <owl:DatatypeProperty rdf:about="http://example.org/person#lastName">
        <rdfs:domain rdf:resource="http://example.org/person#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">Last Name</rdfs:label>
        <rdfs:comment xml:lang="en">The person's family name</rdfs:comment>
    </owl:DatatypeProperty>
    
    <!-- age property -->
    <owl:DatatypeProperty rdf:about="http://example.org/person#age">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://example.org/person#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:label xml:lang="en">Age</rdfs:label>
        <rdfs:comment xml:lang="en">The person's age in years</rdfs:comment>
    </owl:DatatypeProperty>
    
    <!-- email property -->
    <owl:DatatypeProperty rdf:about="http://example.org/person#email">
        <rdfs:domain rdf:resource="http://example.org/person#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">Email</rdfs:label>
        <rdfs:comment xml:lang="en">The person's email address</rdfs:comment>
    </owl:DatatypeProperty>
    
    <!-- employeeId property -->
    <owl:DatatypeProperty rdf:about="http://example.org/person#employeeId">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://example.org/person#Employee"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">Employee ID</rdfs:label>
        <rdfs:comment xml:lang="en">The employee's unique identifier</rdfs:comment>
    </owl:DatatypeProperty>
    
    <!-- street property -->
    <owl:DatatypeProperty rdf:about="http://example.org/person#street">
        <rdfs:domain rdf:resource="http://example.org/person#Address"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">Street</rdfs:label>
    </owl:DatatypeProperty>
    
    <!-- city property -->
    <owl:DatatypeProperty rdf:about="http://example.org/person#city">
        <rdfs:domain rdf:resource="http://example.org/person#Address"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">City</rdfs:label>
    </owl:DatatypeProperty>
    
    <!-- postalCode property -->
    <owl:DatatypeProperty rdf:about="http://example.org/person#postalCode">
        <rdfs:domain rdf:resource="http://example.org/person#Address"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">Postal Code</rdfs:label>
    </owl:DatatypeProperty>
    
    <!-- Object Properties -->
    
    <!-- hasAddress property -->
    <owl:ObjectProperty rdf:about="http://example.org/person#hasAddress">
        <rdfs:domain rdf:resource="http://example.org/person#Person"/>
        <rdfs:range rdf:resource="http://example.org/person#Address"/>
        <rdfs:label xml:lang="en">Has Address</rdfs:label>
        <rdfs:comment xml:lang="en">The person's postal address</rdfs:comment>
    </owl:ObjectProperty>
    
    <!-- worksFor property -->
    <owl:ObjectProperty rdf:about="http://example.org/person#worksFor">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://example.org/person#Employee"/>
        <rdfs:range rdf:resource="http://example.org/person#Organization"/>
        <rdfs:label xml:lang="en">Works For</rdfs:label>
        <rdfs:comment xml:lang="en">The organization the employee works for</rdfs:comment>
    </owl:ObjectProperty>
    
    <!-- hasEmployee property (inverse of worksFor) -->
    <owl:ObjectProperty rdf:about="http://example.org/person#hasEmployee">
        <owl:inverseOf rdf:resource="http://example.org/person#worksFor"/>
        <rdfs:domain rdf:resource="http://example.org/person#Organization"/>
        <rdfs:range rdf:resource="http://example.org/person#Employee"/>
        <rdfs:label xml:lang="en">Has Employee</rdfs:label>
        <rdfs:comment xml:lang="en">The employees of the organization</rdfs:comment>
    </owl:ObjectProperty>
    
    <!-- Restrictions (Example: Person must have at least 1 firstName) -->
    <owl:Class rdf:about="http://example.org/person#Person">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.org/person#firstName"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.org/person#lastName"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    
    <!-- Named Individuals (Examples) -->
    
    <owl:NamedIndividual rdf:about="http://example.org/person#JohnDoe">
        <rdf:type rdf:resource="http://example.org/person#Employee"/>
        <firstName>John</firstName>
        <lastName>Doe</lastName>
        <age rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">35</age>
        <email>john.doe@example.com</email>
        <employeeId>EMP001</employeeId>
        <rdfs:label>John Doe</rdfs:label>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://example.org/person#ExampleOrg">
        <rdf:type rdf:resource="http://example.org/person#Organization"/>
        <rdfs:label>Example Organization</rdfs:label>
    </owl:NamedIndividual>
    
</rdf:RDF>